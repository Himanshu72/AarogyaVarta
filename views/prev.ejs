<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>

    <%- include('common/links'); %>
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.jsdelivr.net/npm/evo-calendar@1.1.2/evo-calendar/css/evo-calendar.min.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="css/evo-calendar.royal-navy.css"
    />
  </head>

  <body>
    <%- include('common/navbar',{auth:data.auth}); %>
    <div class="container">
      <br />
      <br />
      <div id="calendar" style="z-index: 1"></div>
      <br />
      <br />
    </div>
    <%- include('common/footer'); %>

    <!-- Add the evo-calendar.js for.. obviously, functionality! -->
    <script src="https://cdn.jsdelivr.net/npm/evo-calendar@1.1.2/evo-calendar/js/evo-calendar.min.js"></script>
    <script>
      $("#calendar").evoCalendar({
        calendarEvents: [
          {
            id: "bHay68s", // Event's ID (required)
            name: "relif from body pain", // Event name (required)
            date: "May/1/2021", // Event date (required)
            type: "holiday", // Event type (required)
            everyYear: true, // Same event every year (optional)
          },
          {
            id: "bHays", // Event's ID (required)
            name: "Covid(Diabetes)", // Event name (required)
            date: "May/5/2021", // Event date (required)
            type: "holiday", // Event type (required)
            everyYear: true, // Same event every year (optional)
          },
        ],
      });
    </script>
  </body>
</html>
